{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blocks-course/team-member",
  "version": "0.1.0",
  "title": "Team Member",
  "category": "widgets",
  "icon": "admin-users",
  "description": "A team member item.",
  "parent": [
    "blocks-course/team-members"
  ],
  "textdomain": "team-member",
