{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "blocks-course/team-members",
	"version": "0.1.0",
	"title": "Team Members",
	"category": "media",
	"icon": "groups",
	"description": "A block for team members",
	"keywords": [ "team", "group" ],
	"supports": {
		"html": false
	},
	"textdomain": "team-members",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"columns": {
			"type": "number",
			"default": 2
		},
		"id": {
			"type": "number"
		},
		"alt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		}
	}
}
